import java.util.ArrayList;
import java.util.Iterator;
/**
 *
 * @author hepta.brendo.p
 */
public class NewClass {
    public static void main(String args[]){
        ArrayList<String> fruits = new ArrayList<String>();
        String result;
        fruits.add("Volvo");
        fruits.add("Ferrari");
        fruits.add("BMW");
        fruits.add("Renault");
        fruits.add("Lamborguini");
        
            Iterator<String> it = fruits.iterator();   //Iterator interage com o arraylist
            
        while(it.hasNext()){   // Aqui o loop o hasNext verifica o total de itens que precisam serem entregues.
            System.out.println(it.next());  // Este item seleciona na sequência cada intereção feita pelo While.
        }
    }
    
}


