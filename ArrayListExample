import java.util.ArrayList;
/**
 *
 * @author hepta.brendo.p
 */
public class NewClass {
    public static void main(String args[]){
        ArrayList<String> fruits = new ArrayList<String>();
        String result;
        fruits.add("Volvo");
        fruits.add("Ferrari");
        fruits.add("BMW");
        fruits.add("Renault");
        fruits.add("Lamborguini");
        
            for(int x = 0; x < fruits.size(); x++){
                result = fruits.get(x);
                System.out.println(result);  
            }
            
            System.out.println("A quantidade de item Ã© " + fruits.size());
    }
    
}
